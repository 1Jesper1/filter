{
    "name": "joomla/console",
    "type": "joomla-package",
    "description": "Joomla Console Package",
    "keywords": ["joomla", "framework", "console"],
    "homepage": "https://github.com/joomla-framework/console",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "~7.0",
        "joomla/application": "~2.0",
        "symfony/console": "~3.4|~4.0"
    },
    "require-dev": {
        "joomla/coding-standards": "~2.0@alpha",
        "joomla/symfony-event-dispatcher-bridge": "~2.0",
        "phpunit/phpunit": "~6.3"
    },
    "suggest": {
        "joomla/symfony-event-dispatcher-bridge": "To bridge symfony/event-dispatcher with joomla/event"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Console\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
