{
    "name": "joomla/router",
    "type": "joomla-package",
    "description": "Joomla Router Package",
    "keywords": ["joomla", "framework", "router"],
    "homepage": "https://github.com/joomla-framework/router",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^5.3.10|~7.0|^8.0",
        "joomla/controller": "~1.0|~2.0",
        "joomla/input": "~1.0|~2.0"
    },
    "require-dev": {
        "joomla/coding-standards": "~2.0@alpha",
        "joomla/test": "~1.0",
        "phpunit/phpunit": "^4.8.35|^5.4.3|~6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Router\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
