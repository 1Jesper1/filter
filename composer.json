{
    "name": "joomla/filter",
    "type": "joomla-package",
    "description": "Joomla Filter Package",
    "keywords": ["joomla", "framework", "filter"],
    "homepage": "https://github.com/joomla-framework/filter",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "~8.1.0",
        "joomla/string": "3.0.x-dev"
    },
    "require-dev": {
        "joomla/language": "3.0.x-dev",
        "phpunit/phpunit": "^9.5.28",
        "squizlabs/php_codesniffer": "~3.7.1"
    },
    "conflict": {
        "joomla/language": "<2.0"
    },
    "suggest": {
        "joomla/language": "Required only if you want to use `OutputFilter::stringURLSafe`."
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Filter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Filter\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev",
            "dev-3.x-dev": "3.0-dev"
        }
    }
}
